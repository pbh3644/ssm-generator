{
  "project": "cscm-slll",
  "packagePath": "com/sydata/cscm/transfer",
  "entityPackage": "com.sydata.cscm.transfer.bean",
  "model": "mybatis",
  "author": "pangbohuan",
  "dbConfig": {
    "driverClass": "com.mysql.jdbc.Driver",
    "username": "root",
    "password": "123456",
    "url": "jdbc:mysql:\/\/192.168.2.228:3306/cscm?serverTimezone=Asia/Shanghai&autoReconnect=true&useUnicode=true&characterEncoding=utf8&allowMultiQueries=true"
  },
  "ignorePrefix": "",
  "templateDir": "/template",
  "groups": [
    {
      "name": "base",
      "prefix": "BASE"
    },
    {
      "name": "inventory",
      "prefix": "INVENTORY"
    },
    {
      "name": "mall",
      "prefix": "MALL"
    },
    {
      "name": "purchase",
      "prefix": "PURCHASE"
    },
    {
      "name": "sec",
      "prefix": "SECURITY"
    },
    {
      "name": "site",
      "prefix": "SITE"
    },
    {
      "name": "log",
      "prefix": "LOG"
    }
  ],
  "mappings": [
    {
      "template": "pojo.ftl",
      "dir": "${project}/${packagePath}/domain",
      "packagePath": "${packagePath}.domain"
    },
    {
      "template": "dto.ftl",
      "dir": "${project}/${packagePath}/dto",
      "packagePath": "${packagePath}",
      "ePadding": "DTO"
    },
    {
      "template": "service_impl.ftl",
      "dir": "${project}/${packagePath}/service/impl",
      "packagePath": "${packagePath}",
      "ePadding": "ServiceImpl"
    },
    {
      "template": "service_spi.ftl",
      "dir": "${project}/${packagePath}/service",
      "packagePath": "${packagePath}",
      "ePadding": "Service"
    },
    {
      "template": "dalMapper.ftl",
      "dir": "${project}/${packagePath}/mapper",
      "packagePath": "${packagePath}",
      "ePadding": "Mapper"
    },
    {
      "template": "mybatis_xml.ftl",
      "dir": "${project}/mappings/${model}",
      "ePadding": "Mapper",
      "packagePath": "${packagePath}",
      "suffix": "xml"
    }
  ]
}
